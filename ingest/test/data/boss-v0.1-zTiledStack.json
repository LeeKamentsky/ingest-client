{
  "schema": {
      "name": "boss-v0.1-schema",
      "validator": "BossValidatorV01"
  },
  "client": {
    "backend": {
      "name": "boss",
      "class": "BossBackend",
      "host": "api.theboss.io",
      "protocol": "https"
    },
    "path_processor": {
      "class": "ingest.plugins.tiled_stack.ZindexTiledStackPathProcessor",
      "params": {
        "root_dir": "",
        "filesystem": "local"
      }
    },
    "tile_processor": {
      "class": "ingest.plugins.tiled_stack.ZindexTiledStackTileProcessor",
      "params": {
        "filesystem": "local",
        "extension": "png",
        "tile_shape": [1024, 1024]
      }
    }
  },
  "database": {
    "collection": "my_col_1",
    "experiment": "my_exp_1",
    "channel": "my_ch_1"
  },
  "ingest_job": {
    "resolution": 0,
    "extent": {
      "x": [0, 512],
      "y": [0, 512],
      "z": [0, 3],
      "t": [0, 1]
    },
    "tile_size": {
      "x": 1024,
      "y": 1024,
      "z": 1,
      "t": 1
    }
  }
}


